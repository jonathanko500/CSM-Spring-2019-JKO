import java.util.Random;
import java.util.Scanner;

public class Growth
{//start

	public static void main(String[] args) 
	{//start
		Scanner scan = new Scanner(System.in);
		int[] list;
		int length,search,num;
		System.out.print("How big of a list do you want?: ");
		length=Integer.parseInt(scan.nextLine());
		System.out.print("What number are you looking for?: ");
		search=Integer.parseInt(scan.nextLine());
		list = new int[length];
		for(int i=0;i<list.length-1;i++)
		{//start
			System.out.print("Enter number: ");
			num=Integer.parseInt(scan.nextLine());
			list[i]=num;
		}//end
		System.out.println(binary(list,search));
	}//end
	public static int binary(int[] x, int y)
	{//start
		int lo=0,hi=x.length-1,mid=0;
		while(lo<=hi)
		{//start
			int need=lo + (hi - lo) / 2;
			if(y<x[mid])
			{
				hi=mid-1;
			}
			else if (y>x[mid])
			{
				lo = mid + 1;
			}
			else
			{
				mid=need;
			}
		}//end
		if(mid==y)
		{
			return mid;
		}
		else
		{
			return -1;
		}
		
	}//end
}//end
