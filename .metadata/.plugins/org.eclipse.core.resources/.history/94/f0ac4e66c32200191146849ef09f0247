import java.util.Random;
import java.util.Scanner;

public class Growth
{//start

	public static void main(String[] args) 
	{//start
		Scanner scan = new Scanner(System.in);
		
		boolean cont = true;
		int[] list;
		int num,length,search,counter=0;
		System.out.print("How big of list do you want?: ");
		length=Integer.parseInt(scan.nextLine());
		list= new int[length];
		System.out.print("What number are you looking for: ");
		search=Integer.parseInt(scan.nextLine());
		while(cont==true)
		{//keep asking for number
			System.out.print("Enter number: ");
			num=Integer.parseInt(scan.nextLine());
			for(int i=0;i<list.length;i++)
			{//add to num to array
				list[counter]=num;
			}//end
			counter++;
			if(counter==list.length)
			{//stop asking for num
				cont=false;
			}//end
		}//end
		System.out.println(binary(list,search));
	}//end
	public static int binary(int[] x, int y)
	{//start
		int lo=0,hi=x.length-1,mid=0;
		while(lo<=hi)
		{//start
			int need=lo + (hi - lo) / 2;
			if(y<x[mid])
			{
				hi=mid-1;
			}
			else if (y>x[mid])
			{
				lo = mid + 1;
			}
			else
			{
				mid=need;
			}
		}//end
		if(mid==y)
		{
			return mid;
		}
		else
		{
			return -1;
		}
		
	}//end
}//end
